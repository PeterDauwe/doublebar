{% for tag in workspace.tags %}
{% if tag.mine %}
%{A1:$SCRIPTPATH/change_to_tag {{workspace.index}} {{tag.index}}:}
%{T1} %{F#f15495}<{{tag.name}}%{F#f15495}> %{F-}
%{A}
{% elsif tag.visible  %}
%{A1:$SCRIPTPATH/change_to_tag {{workspace.index}} {{tag.index}}:}
%{T1} %{F#bd93f9}.{{tag.name}}%{F#bd93f9}. %{F-}
%{A}
{% elsif tag.busy  %}
%{A1:$SCRIPTPATH/change_to_tag {{workspace.index}} {{tag.index}}:}
%{T1} %{F#f8f8f2}.{{tag.name}}. %{F-}
%{A}
{% else tag.visible  %}
%{A1:$SCRIPTPATH/change_to_tag {{workspace.index}} {{tag.index}}:}
%{T1}%{F#bbbbbb}  {{tag.name}}  %{F-}
%{A}
{% endif %}
{% endfor %}
